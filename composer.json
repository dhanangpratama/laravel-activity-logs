{
    "name": "dhanangpratama/laravel-activity-logs",
    "description": "Activity logs package for laravel framework",
    "type": "library",
    "version": "1.0.0-stable",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0"
    },
    "authors": [
        {
            "name": "Dhanang Pratama",
            "email": "dhanangarfian@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "DhanangPratama\\ActivityLogs\\" : "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DhanangPratama\\ActivityLogs\\Providers\\ActivityLogsServiceProvider"
            ],
            "aliases": {
                "ActivityLogs": "DhanangPratama\\ActivityLogs\\Facades\\ActivityLogs"
            }
        }
    }
}
